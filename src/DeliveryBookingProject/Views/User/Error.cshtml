@if (TempData.Peek("ErrMsg").ToString() == "Incorrect UserName or Password")
{
    @TempData.Remove("ErrMsg")
    <script>
        alert("Incorrect UserName or Password");
        window.location.href = "Login";
    </script>
}
else if (TempData.Peek("ErrMsg").ToString() == "Your Account was Not Verified")
{
    @TempData.Remove("ErrMsg");
    <script>
        alert("Your Account was Not Verified");
        window.location.href = "Login";
    </script>
}
else if (TempData.Peek("ErrMsg").ToString() == "Please Enter Password")
{
    @TempData.Remove("ErrMsg");
    <script>
        alert("Please Enter Admin Password");
        window.location.href = "Login";
    </script>
}



