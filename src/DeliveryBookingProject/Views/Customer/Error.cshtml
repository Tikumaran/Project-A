@if (TempData.Peek("Msg").ToString() == "UserName Exists")
{
    @TempData.Remove("Msg")
    <script>
        alert("UserName is Already Exists Try Other");
        window.location.href = "Register";
    </script>
}
else if (TempData.Peek("Msg").ToString() == "Booking Is Exists")
{

    @TempData.Remove("Msg")
    <script>
        alert("Already Booked Executive in Same date");
        window.location.href = "BookingExecutive";
    </script>
}
else if (TempData.Peek("Msg").ToString() == "Not Eligible")
{

    @TempData.Remove("Msg")
    <script>
        alert("You Are Not Eligible for this Order");
        window.location.href = "Home";
    </script>
}
else if (TempData.Peek("Msg").ToString() == "Not Registered")
{

    @TempData.Remove("Msg")
    <script>
        alert("Registration Not Succeed");
        window.location.href = "Home";
    </script>
}
else if (TempData.Peek("Msg").ToString() == "Not BookingAdded")
{

    @TempData.Remove("Msg")
    <script>
        alert("Booking Not Succeed");
        window.location.href = "BookingExecutive";
    </script>
}
else if (TempData.Peek("Msg").ToString() == "Booking Is IN Error Check Log")
{

    @TempData.Remove("Msg")
    <script>
        alert("Booking Not Succeed Check Log");
        window.location.href = "BookingExecutive";
    </script>
}
else if (TempData.Peek("Msg").ToString() == "No Executives")
{

    @TempData.Remove("Msg")
    <script>
        alert("No Executives");
        window.location.href = "BookingExecutive";
    </script>
}
else if (TempData.Peek("Msg").ToString() == "Selecting Invalid Executive")
{

    @TempData.Remove("Msg")
    <script>
        alert("Selecting Invalid Executive");
        window.location.href = "BookingExecutive";
    </script>
}

