@if (TempData.Peek("Msg").ToString() == "UserName Exists")
{

    @TempData.Remove("Msg")
    <script>
        alert("UserName is Already Exists Try Other");
        window.location.href = "Register";
    </script>
}
else if (TempData.Peek("Msg").ToString() == "Not Eligible")
{

    @TempData.Remove("Msg")
    <script>
        alert("You Are Not Eligible for this Booking");
        window.location.href = "Home";
    </script>
}
else if (TempData.Peek("Msg").ToString() == "Order Not Found")
{

    @TempData.Remove("Msg")
    <script>
        alert("Order Not Found");
        window.location.href = "BookingRequests";
    </script>
}


